{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-curl": "^7.2",
        "deanblackborough/php-quill-renderer": "^3.18",
        "embed/embed": "^3.4",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "highideas/laravel-users-online": "^3.0",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "joedixon/laravel-translation": "^1.1",
        "laracasts/utilities": "^3.0",
        "laravel-notification-channels/pusher-push-notifications": "^1.1",
        "laravel/framework": "^6.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^3.2",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.0",
        "nesbot/carbon": "^2.25",
        "paquettg/php-html-parser": "^2.2",
        "phpdocumentor/graphviz": "^1.0",
        "pusher/pusher-php-server": "^4.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/db-dumper": "^2.15"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpro/grumphp": "dev-master",
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/DateHelper.php",
            "app/Helpers/Internet.php"

        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }

}
